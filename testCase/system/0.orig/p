FoamFile
{
    version     1812;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions              [1 -1 -2 0 0 0 0];

internalField           uniform 101325;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }

//    "(top|bottom|left|right)"
    outer
    {
        type            waveTransmissive;
        field           p;
        phi             phi;
        rho             rho;
        psi             thermo:psi;
        gamma           1.4;
        value           $internalField;
    }
}

// ************************************************************************* //
